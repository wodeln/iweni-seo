{
    "name": "slimkit/plus-appversion",
    "description": "A \"slimkit/plus-appversion\" package For ThinkSNS+✈️.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "illuminate/console": "5.7.*",
        "illuminate/contracts": "5.7.*",
        "illuminate/database": "5.7.*",
        "illuminate/support": "5.7.*"
    },
    "autoload": {
        "psr-4": {
            "Slimkit\\PlusAppversion\\": "src/",
            "Slimkit\\PlusAppversion\\Seeds\\": "database/seeds/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slimkit\\PlusAppversion\\Providers\\AppServiceProvider",
                "Slimkit\\PlusAppversion\\Providers\\ModelServiceProvider",
                "Slimkit\\PlusAppversion\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "version": "2.2.1"
}
