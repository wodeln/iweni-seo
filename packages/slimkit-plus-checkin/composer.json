{
    "name": "slimkit/plus-checkin",
    "description": "✈️The package is a ThinkSNS+ package.",
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.0",
        "illuminate/console": "5.7.*",
        "illuminate/contracts": "5.7.*",
        "illuminate/database": "5.7.*",
        "illuminate/support": "5.7.*"
    },
    "autoload": {
        "psr-4": {
            "SlimKit\\PlusCheckIn\\": "src/",
            "SlimKit\\PlusCheckIn\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlimKit\\PlusCheckIn\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SlimKit\\PlusCheckIn\\Providers\\AppServiceProvider",
                "SlimKit\\PlusCheckIn\\Providers\\ModelServiceProvider",
                "SlimKit\\PlusCheckIn\\Providers\\AuthServiceProvider",
                "SlimKit\\PlusCheckIn\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "version": "2.2.1"
}
